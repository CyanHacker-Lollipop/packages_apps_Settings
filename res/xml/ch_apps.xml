<PreferenceScreen
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:title="@string/ch_apps"
    xmlns:settings="http://schemas.android.com/apk/res/com.android.settings">

	<!-- BetterBatteryStats -->
	<PreferenceScreen 
		android:icon="@drawable/ic_ch_control" 
		android:title="@string/bbs">
        <intent android:targetPackage="com.asksven.betterbatterystats_xdaedition" 
			android:action="android.intent.action.VIEW" 
			android:targetClass="com.asksven.betterbatterystats.StatsActivity" />
    </PreferenceScreen>

    <!-- FolderMount -->	
	<PreferenceScreen 
		android:icon="@drawable/ic_ch_control" 
		android:title="@string/fmount">
        <intent android:targetPackage="com.devasque.fmount" 
			android:action="android.intent.action.VIEW" 
			android:targetClass="com.devasque.fmount.FolderpairActivity" />
    </PreferenceScreen>

    <!-- SDMaid -->	
    <PreferenceScreen 
		android:icon="@drawable/ic_ch_control" 
		android:title="@string/sdmaid">
        <intent android:targetPackage="eu.thedarken.sdm" 
			android:action="android.intent.action.MAIN" 
			android:targetClass="eu.thedarken.sdm.SDMMainActivity" />
    </PreferenceScreen>

    <!-- Greenify -->	
    <PreferenceScreen 
		android:icon="@drawable/ic_ch_control" 
		android:title="@string/greenify">
        <intent android:targetPackage="com.oasisfeng.greenify" 
			android:action="android.intent.action.MAIN" 
			android:targetClass="com.oasisfeng.greenify.GreenifyActivity" />
    </PreferenceScreen>

    <!-- Titanium -->	
    <PreferenceScreen 
		android:icon="@drawable/ic_ch_control" 
		android:title="@string/titanium">
        <intent android:targetPackage="com.keramidas.TitaniumBackup" 
			android:action="android.intent.action.MAIN" 
			android:targetClass="com.keramidas.TitaniumBackup.MainActivity" />
    </PreferenceScreen>
	
    <!-- Wallpaper -->  
    <PreferenceScreen 
       android:icon="@drawable/ic_ch_control"
       android:title="@string/wallpaper">
       <intent  android:targetPackage="com.musenkishi.wally" 
			android:action="android.intent.action.MAIN" 
			android:targetClass="com.musenkishi.wally.activities.MainActivity" />
	</PreferenceScreen > 

</PreferenceScreen>